{
  "name": "alibabacloud/pds-credentials",
  "description": "Alibaba Cloud PDS Credentials Library for PHP",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [],
  "require": {
    "php": ">5.5",
    "alibabacloud/tea": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35|^5.4.3"
  },
  "autoload": {
    "psr-4": {
      "Aliyun\\PDS\\Credentials\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aliyun\\PDS\\Credentials\\Tests\\": "tests"
    }
  },
  "scripts": {
    "fixer": "php-cs-fixer fix ./",
    "test": [
      "@clearCache",
      "phpunit --colors=always"
    ]
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "prefer-stable": true
}
